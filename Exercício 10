using System;

class Program
{
    static void Main()
    { 
     // Exercício 10
        Console.WriteLine("Exercício 10");
        Console.Write("Digite a nota P1: ");
        double p1 = double.Parse(Console.ReadLine());
        Console.Write("Digite a nota P2: ");
        double p2 = double.Parse(Console.ReadLine());
        double media10 = (p1 + 2 * p2) / 3;
        Console.WriteLine($"Média: {media10:F2}");
        Console.WriteLine(media10 >= 5 ? "Aprovado\n" : "Reprovado\n");
    }
}
